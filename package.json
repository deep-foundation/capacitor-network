{
  "name": "@deep-foundation/capacitor-network",
  "type": "module",
  "keywords": [
    "deep-package"
  ],
  "version": "0.1.3",
  "description": "A deep package that is based on @capacitor/network package",
  "repository": {
    "type": "git",
    "url": "https://github.com/deep-foundation/capacitor-network"
  },
  "scripts": {
    "library:build:clean": "fse remove dist && npm run library:build:dirty",
    "library:build:dirty": "ts-node --esm ./automation/library/build.ts",
    "npm-pull": "npm-pull",
    "npm-release": "npm-release",
    "library:build:generate-package-class": "generate-package-class",
    "documentation:generate": "ts-node --esm ./automation/documentation/generate.ts",
    "library:documentation:generate": "typedoc ./src/main.ts"
  },
  "main": "dist/main.js",
  "types": "dist/main.d.ts",
  "files": [
    "/dist",
    "./deep.json"
  ],
  "license": "Unlicense",
  "dependencies": {
    "@deep-foundation/core": "~0.0.2",
    "@deep-foundation/capacitor-device": "~14.0.3",
    "@freephoenix888/boolean": "~0.0.1"
  },
  "peerDependencies": {
    "@capacitor/network": "^5.0.6",
    "@chakra-ui/react": "^2.5.5",
    "@deep-foundation/deeplinks": "~0.0.271",
    "react": "^18.2.0",
    "react-device-detect": "^2.2.3"
  },
  "devDependencies": {
    "@atao60/fse-cli": "^0.1.7",
    "@capacitor/cli": "^5.0.0",
    "@deep-foundation/npm-automation": "^18.1.3",
    "@types/fs-extra": "^11.0.1",
    "@types/node": "^20.2.4",
    "fs-extra": "^11.1.1",
    "typescript": "^4.9.5"
  }
}
